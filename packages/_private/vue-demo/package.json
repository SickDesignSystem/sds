{
  "name": "vue-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "_build": "run-p type-check \"_build-only {@}\" --",
    "preview": "vite preview",
    "_build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format.eslint": "pnpm lint --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@synergy-design-system/assets": "workspace:*",
    "@synergy-design-system/components": "workspace:*",
    "@synergy-design-system/tokens": "workspace:*",
    "@synergy-design-system/vue": "workspace:*",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.9",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.4",
    "typescript": "~5.2.0",
    "vite": "^4.5.2",
    "vite-plugin-static-copy": "^1.0.0",
    "vue-tsc": "^1.8.27"
  },
  "type": "module"
}
