{
  "author": {
    "name": "SICK Global UX Foundation",
    "url": "https://www.sick.com"
  },
  "description": "Assets for the Synergy Design System",
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@synergy-design-system/eslint-config-syn": "workspace:*",
    "@tokens-studio/sd-transforms": "^0.11.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "figma-export-assets": "github:mariohamann/figma-export-assets",
    "rimraf": "^5.0.1",
    "semantic-release": "^19.0.5",
    "semantic-release-monorepo": "^7.0.5",
    "style-dictionary": "^3.8.0",
    "svgo": "^3.0.2"
  },
  "exports": {
    ".": "./build/js/index.js"
  },
  "homepage": "https://github.com/SickDesignSystem/synergy/tree/main/packages/assets",
  "keywords": [
    "Static Assets",
    "SDS",
    "Synergy Design System",
    "SICK",
    "SICK Design System",
    "SICK Icons",
    "SICK Logos"
  ],
  "files": [
    "src/logos",
    "CHANGELOG.md"
  ],
  "license": "BSD-3-Clause",
  "name": "@synergy-design-system/assets",
  "repository": {
    "type": "git",
    "url": "https://github.com/SickDesignSystem/synergy.git",
    "directory": "packages/assets"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      {
        "name": "main",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      },
      "release-fallback"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release/assets): ${nextRelease.version} [skip actions]\n\n${nextRelease.notes}",
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "fetch-assets": "node scripts/fetch-assets.js",
    "release": "semantic-release  --tagFormat 'assets/${version}' -e semantic-release-monorepo"
  },
  "type": "module",
  "types": "./build/js/index.d.ts",
  "version": "1.0.0-main.1"
}
