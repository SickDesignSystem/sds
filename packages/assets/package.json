{
  "author": {
    "name": "SICK Integrated Automation - UX and UI Services Team",
    "url": "https://www.sick.com"
  },
  "description": "Assets for the SICK Design System",
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sick-design-system/eslint-config-sds": "workspace:^",
    "@tokens-studio/sd-transforms": "^0.11.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "figma-export-assets": "github:mariohamann/figma-export-assets",
    "rimraf": "^5.0.1",
    "semantic-release": "^22.0.5",
    "semantic-release-monorepo": "^7.0.5",
    "style-dictionary": "^3.8.0",
    "svgo": "^3.0.2"
  },
  "exports": {
    ".": "./build/js/index.js"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/SickDesignSystem/sds/tree/main/packages/assets",
  "keywords": [
    "SDS",
    "SICK",
    "SICK Design System",
    "Design Tokens"
  ],
  "license": "BSD-3-Clause",
  "name": "@sick-design-system/assets",
  "repository": {
    "type": "git",
    "url": "https://github.com/SickDesignSystem/sds.git",
    "directory": "packages/assets"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release/assets): ${nextRelease.version} [skip actions]\n\n${nextRelease.notes}",
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "fetch-assets": "node scripts/fetch-assets.js"
  },
  "type": "module",
  "types": "./build/js/index.d.ts",
  "version": "0.1.0"
}
