{
  "author": {
    "name": "SICK Global UX Foundation",
    "url": "https://www.sick.com"
  },
  "dependencies": {
    "@lit/react": "^1.0.0",
    "@synergy-design-system/components": "workspace:*"
  },
  "devDependencies": {
    "@synergy-design-system/eslint-config-syn": "workspace:*",
    "@synergy-design-system/tokens": "workspace:*",
    "@types/react": "^18.2.30",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.0"
  },
  "description": "React wrappers the Synergy Design System",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/*": "./dist/components/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "homepage": "https://github.com/SickDesignSystem/synergy/tree/main/packages/react",
  "keywords": [
    "React Web Component Wrapper",
    "React Components",
    "React Design System",
    "Design System",
    "SDS",
    "Synergy Design System",
    "SICK",
    "SICK Design System"
  ],
  "license": "BSD-3-Clause",
  "name": "@synergy-design-system/react",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      {
        "name": "main",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      },
      "release-fallback"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release/components): ${nextRelease.version} [skip actions]\n\n${nextRelease.notes}",
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SickDesignSystem/synergy.git",
    "directory": "packages/react"
  },
  "scripts": {
    "lint": "eslint \"demo/**/*.{js,ts,tsx}\" --ext js,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "release": "semantic-release --tagFormat 'react/${version}' -e semantic-release-monorepo",
    "start": "vite"
  },
  "type": "module",
  "version": "1.0.0-main.1"
}
