{
  "author": {
    "name": "SICK Global UX Foundation",
    "url": "https://www.sick.com"
  },
  "dependencies": {
    "@lit/react": "^1.0.0",
    "@synergy-design-system/components": "workspace:*"
  },
  "description": "React wrappers for the Synergy Design System",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/*": "./dist/components/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "homepage": "https://github.com/SickDesignSystem/synergy/tree/main/packages/react",
  "keywords": [
    "React Web Component Wrapper",
    "React Components",
    "React Design System",
    "Design System",
    "SDS",
    "Synergy Design System",
    "SICK",
    "SICK Design System"
  ],
  "license": "BSD-3-Clause",
  "name": "@synergy-design-system/react",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      {
        "name": "main",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      },
      "release-fallback"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release/components): ${nextRelease.version} [skip actions]\n\n${nextRelease.notes}",
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SickDesignSystem/synergy.git",
    "directory": "packages/react"
  },
  "type": "module",
  "version": "1.0.0-main.1",
  "devDependencies": {
    "@types/react": "^18.2.31",
    "react": "^18.2.0"
  }
}
